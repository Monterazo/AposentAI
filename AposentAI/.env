# Project name
PROJECT_NAME=AposentAI

# Stack name
STACK_NAME=aposentai

# Secret key for security
SECRET_KEY=your-secret-key-here-change-this-in-production

# First superuser
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changethis

# SMTP settings (optional for local development)
SMTP_HOST=
SMTP_USER=
SMTP_PASSWORD=
EMAILS_FROM_EMAIL=info@example.com

# PostgreSQL database
POSTGRES_PASSWORD=changethis
POSTGRES_USER=postgres
POSTGRES_DB=app
POSTGRES_PORT=5432

# Sentry DSN (optional)
SENTRY_DSN=

# Domain for local development
DOMAIN=localhost

# Frontend host
FRONTEND_HOST=http://localhost:5173

# Environment
ENVIRONMENT=local

# Backend CORS origins
BACKEND_CORS_ORIGINS=["http://localhost:5173", "http://localhost:3000", "http://localhost:8080"]

# Docker image names
DOCKER_IMAGE_BACKEND=aposentai-backend
DOCKER_IMAGE_FRONTEND=aposentai-frontend

# Tag for Docker images
TAG=latest
